{
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "src"
        ],
        "excludePattern": "(node_modules/|docs)"
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "destination": "./doc",
        "recurse": true
    }
}